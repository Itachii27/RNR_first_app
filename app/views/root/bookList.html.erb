<%= render "root/header" %>

<table class="table table-sm">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
    </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>

    <tr>
      <td><%= post.title %></td>
            <td><%= post.description %></td>

    </tr>
      <% end %>

  </tbody>
</table>



